[Unit]
Description=ChirpStack Concentratord SX1301
Documentation=https://www.chirpstack.io/
Wants=network-online.target
After=network-online.target

[Service]
User=chirpstack
Group=chirpstack
ExecStart=/usr/bin/chirpstack-concentratord-sx1301 -c /etc/chirpstack-concentratord/chirpstack-concentratord-sx1301.toml
Restart=on-failure

[Install]
WantedBy=multi-user.target
